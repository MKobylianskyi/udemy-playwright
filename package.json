{
  "name": "udemy-playwright",
  "version": "1.0.0",
  "description": "testing project",
  "main": "index.js",
  "scripts": {
    "test:BYOE": "playwright test tests/BYOE --config=playwright.config.ts --project=Chromium --repeat-each 1",
    "test:example": "playwright test tests/example.spec.ts --config=playwright.config.ts  --project=Chromium",
    "test:e2e": "playwright test --config=e2e.config.ts --project=Chromium",
    "test:visual": "playwright test --config=visual.config.ts --project=Chromium",
    "test:api": "playwright test --config=api.config.ts --project=Chromium",
    "test:browser": "npx playwright open --timezone='Europe/Rome' --lang='it-IT' google.com"
  },
  "author": "Mykhailo",
  "license": "ISC",
  "devDependencies": {
    "prettier": "^2.6.2"
  },
  "dependencies": {
    "@playwright/test": "^1.21.1"
  }
}
